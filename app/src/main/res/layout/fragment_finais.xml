<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="br.com.futeboldospais.futeboldospais.controller.FinaisFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <RelativeLayout
                android:id="@+id/titulo_tabela"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/titulo"
                android:background="@drawable/borda_inferior"
                android:gravity="center"
                android:textColor="@color/grayLight">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="50dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:gravity="center"
                    android:text="SEMIFINAIS" />

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@id/titulo_tabela"
                android:layout_marginStart="20dp">

                <LinearLayout
                    android:id="@+id/time_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="30dp"
                    android:background="@drawable/borda_inferior"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/img_master_principal_1"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:padding="5dp" />

                    <TextView
                        android:id="@+id/txt_master_principal_1"
                        android:layout_width="120dp"
                        android:layout_height="40dp"
                        android:gravity="center" />

                    <TextView
                        android:id="@+id/gols_master_principal_1"
                        android:layout_width="30dp"
                        android:layout_height="40dp"
                        android:gravity="center" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/time_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/time_1"
                    android:background="@drawable/borda_inferior"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/img_master_repescagem_1"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:padding="5dp" />

                    <TextView
                        android:id="@+id/txt_master_repescagem_1"
                        android:layout_width="120dp"
                        android:layout_height="40dp"
                        android:background="@drawable/borda_inferior"
                        android:gravity="center" />

                    <TextView
                        android:id="@+id/gols_master_repescagem_1"
                        android:layout_width="30dp"
                        android:layout_height="40dp"
                        android:gravity="center" />
                </LinearLayout>

                <View
                    android:id="@+id/divisor_1"
                    android:layout_width="70dp"
                    android:layout_height="2dp"
                    android:layout_below="@id/time_1"
                    android:layout_toEndOf="@+id/time_1"
                    android:background="#A8A8A8" />

                <View
                    android:id="@+id/divisor_2"
                    android:layout_width="2dp"
                    android:layout_height="100dp"
                    android:layout_below="@id/divisor_1"
                    android:layout_toEndOf="@+id/divisor_1"
                    android:background="#A8A8A8" />

                <LinearLayout
                    android:id="@+id/time_3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/time_2"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/borda_inferior"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/img_master_principal_2"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:padding="5dp" />

                    <TextView
                        android:id="@+id/txt_master_principal_2"
                        android:layout_width="120dp"
                        android:layout_height="40dp"
                        android:background="@drawable/borda_inferior"
                        android:gravity="center" />

                    <TextView
                        android:id="@+id/gols_master_principal_2"
                        android:layout_width="30dp"
                        android:layout_height="40dp"
                        android:gravity="center" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/time_4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/time_3"
                    android:background="@drawable/borda_inferior"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/img_master_principal_3"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:padding="5dp" />

                    <TextView
                        android:id="@+id/txt_master_principal_3"
                        android:layout_width="120dp"
                        android:layout_height="40dp"
                        android:background="@drawable/borda_inferior"
                        android:gravity="center" />

                    <TextView
                        android:id="@+id/gols_master_principal_3"
                        android:layout_width="30dp"
                        android:layout_height="40dp"
                        android:gravity="center" />
                </LinearLayout>

                <View
                    android:id="@+id/divisor_3"
                    android:layout_width="70dp"
                    android:layout_height="2dp"
                    android:layout_below="@id/time_3"
                    android:layout_toEndOf="@+id/time_3"
                    android:background="#A8A8A8" />


                <LinearLayout
                    android:id="@+id/time_5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/time_4"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/borda_inferior"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/img_senior_principal_1"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:padding="5dp" />

                    <TextView
                        android:id="@+id/txt_senior_principal_1"
                        android:layout_width="120dp"
                        android:layout_height="40dp"
                        android:background="@drawable/borda_inferior"
                        android:gravity="center" />

                    <TextView
                        android:id="@+id/gols_senior_principal_1"
                        android:layout_width="30dp"
                        android:layout_height="40dp"
                        android:gravity="center" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/time_6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/time_5"
                    android:background="@drawable/borda_inferior"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/img_senior_repescagem_1"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:padding="5dp" />

                    <TextView
                        android:id="@+id/txt_senior_repescagem_1"
                        android:layout_width="120dp"
                        android:layout_height="40dp"
                        android:background="@drawable/borda_inferior"
                        android:gravity="center" />

                    <TextView
                        android:id="@+id/gols_senior_repescagem_1"
                        android:layout_width="30dp"
                        android:layout_height="40dp"
                        android:gravity="center" />
                </LinearLayout>

                <View
                    android:id="@+id/divisor_4"
                    android:layout_width="70dp"
                    android:layout_height="2dp"
                    android:layout_below="@id/time_5"
                    android:layout_toEndOf="@+id/time_5"
                    android:background="#A8A8A8" />

                <View
                    android:id="@+id/divisor_5"
                    android:layout_width="2dp"
                    android:layout_height="100dp"
                    android:layout_below="@id/divisor_4"
                    android:layout_toEndOf="@+id/divisor_4"
                    android:background="#A8A8A8" />

                <LinearLayout
                    android:id="@+id/time_7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/time_6"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/borda_inferior"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/img_senior_principal_2"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:padding="5dp" />

                    <TextView
                        android:id="@+id/txt_senior_principal_2"
                        android:layout_width="120dp"
                        android:layout_height="40dp"
                        android:background="@drawable/borda_inferior"
                        android:gravity="center" />

                    <TextView
                        android:id="@+id/gols_senior_principal_2"
                        android:layout_width="30dp"
                        android:layout_height="40dp"
                        android:gravity="center" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/time_8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/time_7"
                    android:background="@drawable/borda_inferior"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/img_senior_principal_3"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:padding="5dp" />

                    <TextView
                        android:id="@+id/txt_senior_principal_3"
                        android:layout_width="120dp"
                        android:layout_height="40dp"
                        android:background="@drawable/borda_inferior"
                        android:gravity="center" />

                    <TextView
                        android:id="@+id/gols_senior_principal_3"
                        android:layout_width="30dp"
                        android:layout_height="40dp"
                        android:gravity="center" />
                </LinearLayout>

                <View
                    android:id="@+id/divisor_6"
                    android:layout_width="70dp"
                    android:layout_height="2dp"
                    android:layout_below="@id/time_7"
                    android:layout_toEndOf="@+id/time_7"
                    android:background="#A8A8A8" />

                <View
                    android:id="@+id/divisor_8"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_below="@id/divisor_1"
                    android:layout_marginTop="50dp"
                    android:layout_toEndOf="@+id/divisor_2"
                    android:background="#A8A8A8" />

                <View
                    android:id="@+id/divisor_9"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_below="@id/divisor_4"
                    android:layout_marginTop="50dp"
                    android:layout_toEndOf="@+id/divisor_5"
                    android:background="#A8A8A8" />

            </RelativeLayout>
        </RelativeLayout>
    </ScrollView>
</RelativeLayout>
